{
  "css.validate": false,
  "scss.validate": false,
  "less.validate": false,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "tailwindCSS.includeLanguages": {
    "css": "css",
    "html": "html"
  },
  "tailwindCSS.experimental.classRegex": [
    "class[Name]*\\s*[:=]\\s*[\"'`]([^\"'`]*)[\"'`]",
    "className\\s*[:=]\\s*[\"'`]([^\"'`]*)[\"'`]"
  ]
}
